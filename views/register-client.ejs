<%- include('partials/partnerHeader') %>


    <div class="jumbotron centred register-card-container">
        <div class= "card register-card">
            <div class="card-header">
                <h2>Register Clients: </h2>
            </div>
            <div class="card-body">
                <form method="post" action="/register-client">
            
                    <div class="form-group">
                        <label for="companyName">Company Name: </label>
                        <input class="form-control" type="text" name="companyName" id="companyName" required placeholder="Enter your Company name"> 
                    </div>
                    
                    <div class="form-group">
                        <label for="ownerName">Owner's Name:  </label>
                        <input class="form-control" type="text" name="ownerName" id="ownerName" required placeholder="Enter your Company name">
                    </div>
        
                    <div class="form-group">
                        <label for="mobileNumber">Contact Number: </label>
                        <input type="number" class="form-control" name="phoneNumber" id="mobileNumber" placeholder="XXXXX XXXXX" required>
                    </div>
        
                    <div class="form-group">
                        <label for="email">Email: </label>
                        <input class="form-control" type="email" name="e-mail" id="email" placeholder="abcd05@gmail.com" required>
                    </div>
                    
                    <div class="form-group">

                        <label for="state">State: </label>
                        <select name="state" class="form-control" id="state" required>
                            <option value="">--select your state--</option>
                            <option value="delhi">DELHI</option>
                            <option value="west bengal">WEST BENGAL</option>
                            <option value="bihar">BIHAR</option>
                            <option value="gujrat">GUJARAT</option>
                            <option value="jharkhand">JHARKHAND</option>
                            <option value="karnataka">KARNATAKA</option>
                        </select>
                    </div>
                    
                    <div class="form-group">

                        <label for="city">City:  </label>
                        <select name="city" class="form-control" id="city" required>
                            <option value="">--select your city--</option>
                            <option value="delhi">DELHI CAPITAL</option>
                            <option value="kolkata">KOLKATA</option>
                            <option value="patna">PATNA</option>
                            <option value="surat">SURAT</option>
                            <option value="ranchi">RANCHI</option>
                            <option value="banglore">BANGLORE</option>
                            <option value="varodra">VARODRA</option>
                        </select>
                    </div>
        
                    <div class="form-group">

                        <label for="address">Address:  </label>
                        <textarea class="form-control" name="address" id="address" cols="110" rows="3" placeholder="Enter your address" required></textarea>
                    </div>
                    
                    <div class="form-group">

                        <label for="area_pin">Pincode:   </label>
                        <input class="form-control" type="number" name="arer_pin" id="area_pin" placeholder="XXXXXX" required>
                    </div>
        
                    <button type="submit" class="btn btn-primary">Submit</button>
                    
                    <button type="reset" class="btn btn-primary">Reset</button>
        
                </form>
            </div>
        </div>
        
        
 </div>

       <%- include('partials/footer') %>
